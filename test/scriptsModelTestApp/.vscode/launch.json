    "version": "0.2.0",
    "configurations": [
    {
        "type": "chrome",
        "request": "launch",
        "name": "Launch Chrome",
        "url": "http://localhost:8080/combined-cshtml.html",
        "debugServer": 4712,
        "webRoot": "${workspaceFolder}/site"
    },
        {
            "type": "node",
            "request": "launch",
            "name": "Combine maps",
            "program": "${workspaceFolder}/composites/manually-combined/out/combine-maps.js",
            "env": {
                "cshtml-ts-first-script.js.map-offset": "1",
                "cshtml-ts-second-script.js.map-offset": "1"
            }
        }
    ]
}